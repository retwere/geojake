{
  "extends": "@tsconfig/recommended/tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "incremental": true,
    "lib": ["ES6"],
    "sourceMap": true,
    "target": "ES6"
  },
  "exclude": [
    "**/lib",
    "**/node_modules",
    "**/test",
    "**/types",
    "**/*.test.ts",
    "**/*.d.ts",
    "**/*.js"
  ],
  "display": "GeoJakeBase"
}

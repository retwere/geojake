{
  "name": "geojake-demo",
  "version": "0.0.0",
  "author": "Jake Fowler <retwere@gmail.com>",
  "description": "A demo of geojake geohashes!",
  "main": "app/index",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "clean": "rm -rf app coverage types *.log *.tsbuildinfo",
    "distclean": "yarn clean && rm -rf node_modules",
    "lint": "yarn eslint *.json src/*.ts src/*.tsx",
    "start": "yarn start:dev",
    "start:dev": "webpack serve --mode development --env development",
    "test": "yarn jest"
  },
  "dependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "geojake": "0.0.0",
    "leaflet": "^1.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "collectCoverage": true
  },
  "devDependencies": {
    "@types/leaflet": "^1.7.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "css-loader": "^5.1.2",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "source-map-loader": "^2.0.1",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}

{
  "name": "geojake",
  "version": "0.0.0",
  "author": "Jake Fowler <retwere@gmail.com>",
  "description": "Useful utilities for geohashing.",
  "main": "lib/main",
  "types": "types/main",
  "keywords": [
    "geohash",
    "geospatial",
    "geocoding",
    "location"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf coverage docs lib types *.log *.tsbuildinfo",
    "distclean": "yarn clean && rm -rf node_modules",
    "docs": "typedoc --plugin typedoc-plugin-markdown src/main.ts",
    "lint": "yarn eslint *.json src/*.ts",
    "test": "yarn jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true
  }
}
